<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="uploadPath" value="BatteryUploads"/>
		<add key="V_threshold" value="0.1"/>
		<add key="Z_threshold" value="0.5"/>
		<add key="DeviationPercent" value="25"/>
	</appSettings>
	<system.serviceModel>
		<client>
			<endpoint name="Battery"
			address="net.tcp://localhost:4100/Battery"
			binding="netTcpBinding"
			bindingConfiguration="streamBinding"
			contract="Common.IBatteryService" />
		</client>
		<bindings>
			<netTcpBinding>
				<binding name="streamBinding" transferMode="Streamed" maxReceivedMessageSize="10485760" openTimeout="00:00:10" closeTimeout="00:00:10" sendTimeout="00:00:30" receiveTimeout="00:10:00" />
			</netTcpBinding>
		</bindings>
	</system.serviceModel>
</configuration>